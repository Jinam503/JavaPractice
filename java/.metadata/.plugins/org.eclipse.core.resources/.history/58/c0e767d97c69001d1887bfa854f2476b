package pack;

import character.Player;
public class Test {
	public static void main(String[] args) {
		int[][] board = new int[10][10];
		Player black = new Player("b", 0);
		Player white = new Player("w", 1);
		
		while(true) {
			for(int i = 0; i< 10; i++) {
				for(int j = 0; j< 10; j++) {
					System.out.print(board[i][j]+" ");
				}
				System.out.println("");
			}
			clearScreen();
		}
		
	}
	public static void clearScreen() {
		 for (int i = 0; i < 80; i++)
		      System.out.println("");
		  
	}
}
