package pack;

import character.Player;
import java.io.IOException;

public class Test {
	public static void main(String[] args) {
		int[][] board = new int[10][10];
		Player black = new Player("b", 0);
		Player white = new Player("w", 1);
		
		while(true) {
			for(int i = 0; i< 10; i++) {
				for(int j = 0; j< 10; j++) {
					System.out.print(".");
				}
				System.out.println("");
			}
			System.out.print("\033[H\033[2J");
	        System.out.flush();
		}
	}
}
